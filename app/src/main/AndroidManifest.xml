<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vaiterapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".StaffMainActivity"
            android:label="@string/title_activity_staff_main"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".CustomerMainActivity"
            android:label="@string/title_activity_customer_main"
            android:theme="@style/Theme.Design.Light.NoActionBar" />
        <activity android:name=".StaffSignupActivity" />
        <activity android:name=".LaunchActivity" />
        <activity
            android:name=".CustomerSignupActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>